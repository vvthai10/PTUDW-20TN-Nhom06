                    <form action="/search" method="GET">
                      {{!-- <div class="header__search-icon"></div> --}}
                      <button class="header_search-icon"></button>
                      <input type="search" class="header__search-input" placeholder="Search" />
                    </form>
                  </div>
                </div>
              </div>
              <div class="header__navbar">
                <div class="header__navbar-box">
                  <div class="header__navbar-login">
                    {{#if userInfo}}
                    {{else}}
                    <ul class="header__navbar-list" id="action-user">
                      <li>
                        <a aria-current="page" class="header__navbar-item" href="./login"><svg aria-hidden="true"
                            focusable="false" data-prefix="fas" data-icon="right-to-bracket"
                            class="svg-inline--fa fa-right-to-bracket" role="img" xmlns="http://www.w3.org/2000/svg"
                            viewBox="0 0 512 512">
                            <path fill="currentColor"
                              d="M344.7 238.5l-144.1-136C193.7 95.97 183.4 94.17 174.6 97.95C165.8 101.8 160.1 110.4 160.1 120V192H32.02C14.33 192 0 206.3 0 224v64c0 17.68 14.33 32 32.02 32h128.1v72c0 9.578 5.707 18.25 14.51 22.05c8.803 3.781 19.03 1.984 26-4.594l144.1-136C354.3 264.4 354.3 247.6 344.7 238.5zM416 32h-64c-17.67 0-32 14.33-32 32s14.33 32 32 32h64c17.67 0 32 14.33 32 32v256c0 17.67-14.33 32-32 32h-64c-17.67 0-32 14.33-32 32s14.33 32 32 32h64c53.02 0 96-42.98 96-96V128C512 74.98 469 32 416 32z">
                            </path>
                          </svg>
                          <span>Đăng nhập</span>
                        </a>
                      </li>
                    </ul>
                    {{/if}}
                  </div>
                  <!--USER INFO-->
                  {{#with userInfo}}
                  <div class="header__navbar-user">
                    <div class="header__navbar-user-info">
                      <div style="position: inherit">
                        <img src="{{avatar}}" alt="avatar user" class="header__navbar-user-img" />
                        {{#ifEquals role "default"}}
                        {{else}}
                        <img src="/assets/images/user_premium.png" alt="logo" class="header__navbar__user-premium" />
                        {{/ifEquals}}
                      </div>
                      <span class="header__navbar-user-name">{{name}}</span>
                    </div>
                    <ul class="header__navbar-list" id="action-user">
                      {{#ifEquals role "admin"}}
                      <li>
                        <a aria-current="page" class="header__navbar-item" href="/admin">Quản trị viên</a>
                      </li>
                      {{/ifEquals}}
                      {{#ifEquals role "editor"}}
                      <li>
                        <a aria-current="page" class="header__navbar-item" href="/editor">Biên tập viên</a>
                      </li>
                      {{/ifEquals}}
                      {{#ifEquals role "writer"}}
                      <li>
                        <a aria-current="page" class="header__navbar-item" href="/writer">Phóng viên</a>
                      </li>
                      {{/ifEquals}}
                      <li>
                        <a aria-current="page" class="header__navbar-item" href="/users/account?view=info">
                          <span>Trang cá nhân</span>
                        </a>
                      </li>
                    </ul>
                  </div>
                  {{/with}}
                  <!--USER INFO-->
                  <div class="header__navbar-premium">
                    <a href="/premium"><img src="/assets/images/premium.png" alt="premium icon"
                        class="header__navbar-premium-icon" /></a>
                  </div>
                  <!--CATEGORIES INFO-->
                  <ul class="header__navbar-list" id="action-find">
                    <li>
                      <a aria-current="page" class="header__navbar-item header__navbar-active" href="/"><svg
                          aria-hidden="true" focusable="false" data-prefix="fas" data-icon="house"
                          class="svg-inline--fa fa-house" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512">
                          <path fill="currentColor"
                            d="M575.8 255.5C575.8 273.5 560.8 287.6 543.8 287.6H511.8L512.5 447.7C512.5 450.5 512.3 453.1 512 455.8V472C512 494.1 494.1 512 472 512H456C454.9 512 453.8 511.1 452.7 511.9C451.3 511.1 449.9 512 448.5 512H392C369.9 512 352 494.1 352 472V384C352 366.3 337.7 352 320 352H256C238.3 352 224 366.3 224 384V472C224 494.1 206.1 512 184 512H128.1C126.6 512 125.1 511.9 123.6 511.8C122.4 511.9 121.2 512 120 512H104C81.91 512 64 494.1 64 472V360C64 359.1 64.03 358.1 64.09 357.2V287.6H32.05C14.02 287.6 0 273.5 0 255.5C0 246.5 3.004 238.5 10.01 231.5L266.4 8.016C273.4 1.002 281.4 0 288.4 0C295.4 0 303.4 2.004 309.5 7.014L564.8 231.5C572.8 238.5 576.9 246.5 575.8 255.5L575.8 255.5z">
                          </path>
                        </svg>
                        <span>Trang chủ</span>
                      </a>
                    </li>
                    {{#each categories}}
                    <li class="header__navbar-item-have-child">
                      <a aria-current="page" class="header__navbar-item" id="have-list-child">
                        <img src="{{this.icon}}" alt="{{this.name}}" style="width: 20px; height:20px">
                        <span style="font-size: 12px; font-weight: 500">{{this.name}}</span>
                      </a>
                      <!--CHILD CATEGORIES INFO-->
                      {{#if SubCategories.length}}
                      <ul class="header__navbar-list-child">
                        {{#each SubCategories}}
                        <li>
                          <a aria-current="page" class="header__navbar-item"
                            href="/subCategory?subCategoryId={{this.id}}">
                            <span style="font-size: 12px; font-weight: 500">{{name}}</span>
                          </a>
                        </li>
                        {{/each}}
                      </ul>
                      {{/if}}
                      <!--CHILD CATEGORIES INFO-->
                    </li>
                    {{/each}}
                  </ul>
                  <!--CATEGORIES INFO-->

                  {{#if userInfo}}
                  <ul class="header__navbar-list" id="action-user">
                    <li>
                      <a aria-current="page" class="header__navbar-item" href="/users/login"><svg aria-hidden="true"
                          focusable="false" data-prefix="fas" data-icon="right-from-bracket"
                          class="svg-inline--fa fa-right-from-bracket" role="img" xmlns="http://www.w3.org/2000/svg"
                          viewBox="0 0 512 512">
                          <path fill="currentColor"
                            d="M96 480h64C177.7 480 192 465.7 192 448S177.7 416 160 416H96c-17.67 0-32-14.33-32-32V128c0-17.67 14.33-32 32-32h64C177.7 96 192 81.67 192 64S177.7 32 160 32H96C42.98 32 0 74.98 0 128v256C0 437 42.98 480 96 480zM504.8 238.5l-144.1-136c-6.975-6.578-17.2-8.375-26-4.594c-8.803 3.797-14.51 12.47-14.51 22.05l-.0918 72l-128-.001c-17.69 0-32.02 14.33-32.02 32v64c0 17.67 14.34 32 32.02 32l128 .001l.0918 71.1c0 9.578 5.707 18.25 14.51 22.05c8.803 3.781 19.03 1.984 26-4.594l144.1-136C514.4 264.4 514.4 247.6 504.8 238.5z">
                          </path>
                        </svg>
                        <span>Đăng xuất</span>
                      </a>
                    </li>
                  </ul>
                  {{/if}}
                </div>
              </div>
              <!--Action-->
              <div class="header__action col-md-2">
                <!--Search in mobile-->
                <a href="#" class="header__search_page">
                  <div class="header__search-icon"></div>
                </a>

                {{#if userInfo}}
                <div class="header__action-notify">
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                    class="bi bi-bell-fill header__action-notify-icon" viewBox="0 0 16 16">
                    <path
                      d="M8 16a2 2 0 0 0 2-2H6a2 2 0 0 0 2 2zm.995-14.901a1 1 0 1 0-1.99 0A5.002 5.002 0 0 0 3 6c0 1.098-.5 6-2 7h14c-1.5-1-2-5.902-2-7 0-2.42-1.72-4.44-4.005-4.901z" />
                  </svg>
                  <div class="header__action-notify-box">
                    <header>
                      <h6 class="header__action-notify-title">Thông báo</h6>
                      <p class="header__action-notify-isread">Đánh dấu đã đọc</p>
                    </header>
                    <ul class="header__action-notify-list">
                      <li>
                        <a href="#" class="header__action-notify-item">
                          <img src="/assets/images/default.jpg " alt="notify icon"
                            class="header__action-notify-item-icon" />
                          <div class="header__action-notify-item-box">
                            <span class="header__action-notify-item-title">Bình luận của Nguyễn Văn A trong bài viết mới
                              nhất
