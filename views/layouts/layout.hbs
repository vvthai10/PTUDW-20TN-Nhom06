            </div>
          </div>
        </div>
      </div>
      <!--HEADER-->

      {{#if header_note}}
      <div style="padding-top: 60px;">
        {{{body}}}
      </div>
      {{else}}
      <!--MAIN-->
      <div class="main">
        <div class="container-main">
          <div class="row" style="width: 100%;">

            <div class="col-12" style=" margin-top: 10px; display: flex; justify-content: center;">
              {{{body}}}
            </div>
            {{#if header_note}}
            {{else}}
            <div class="sidebar-container">
              <!--SIDE BAR-->
              <div class="sidebar">
                <div class="sidebar__wrapper">
                  <ul class="sidebar__list">
                    <li style="margin: 10px 0">
                      <a aria-current="page" class="sidebar__item sidebar__active" href="/">
                        <img src="/assets/images/icons/trang_chu.png" alt="trang_chu" style="width: 40px; height:40px">
                        <span style="font-size: 12px; font-weight: 500">Trang chủ</span>
                      </a>
                    </li>
                    {{#each categories}}
                    <li class="sidebar__item-have-child" style="margin: 10px 0">
                      <a aria-current="page" class="sidebar__item" id="have-list-child"
